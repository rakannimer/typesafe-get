{
  "name": "typesafe-get",
  "version": "0.2.0",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "npm run test:i",
    "build": "rm -rf ./dist/ && rollup -c",
    "test:i": "jest --watch --coverage",
    "test": "jest --coverage",
    "prepublish": "npm run test && npm run build",
    "commit": "git-cz",
    "setup:semantic-release": "npx semantic-release-cli setup"
  },
  "husky": {
    "pre-commit": "prettier src/* --write",
    "pre-push": "npm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rakannimer/typesafe-get"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.12",
    "comment-json": "^1.1.3",
    "git-cz": "^3.0.1",
    "husky": "^2.2.0",
    "jest": "^24.8.0",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.17.0",
    "rollup": "^1.11.3",
    "rollup-plugin-typescript2": "^0.21.0",
    "semantic-release": "^15.13.3",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.5"
  },
  "jest": {
    "preset": "ts-jest",
    "collectCoverageFrom": [
      "src/*.{ts,tsx}"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/build/",
      "/dist/"
    ]
  },
  "files": [
    "dist/"
  ],
  "description": "Safely drill for nested values"
}
